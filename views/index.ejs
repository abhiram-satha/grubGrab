    <!-- Font Awesome Library -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css"
      integrity="sha512-Fo3rlrZj/k7ujTnHg4CGR2D7kSs0v4LLanw2qksYuRlEzO+tcaEPQogQ0KaoGN26/zrn20ImR1DfuLWnOo7aBA=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />

    <!-- GrubHub Style Sheets -->
    <link rel="stylesheet" href="/styles/cart.css" />
    <link rel="stylesheet" href="/styles/nav.css" />
    <link rel="stylesheet" href="/styles/header.css" />
    <link rel="stylesheet" href="/styles/ourlayout.css" />

    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>

    <!-- GrubHub Style Sheets -->
    <script type="text/javascript" src="/scripts/cart.js"></script>
  </head>

  <body>
    <!-- Top nav bar (fixed) -->

    <nav class="navbar">
      <div class="logo">
        <img class="grublogo" src="./logo4.png" alt="Grub Grab" />
      </div>
      <div class="cart">
        <div>0</div>
        <i class="fas fa-shopping-cart"></i>
      </div>
    </nav>

    <div class="orders">
      <h4>Cart Items</h4>
      <table class="orders-list">
        <tr>
          <th class="cart-food-photo"></th>
          <th class="cart-food-name">Item</th>
          <th class="cart-food-quantity">Quantity</th>
          <th class="cart-food-cost">Cost</th>
          <th class="cart-food-remove">Remove</th>
        </tr>
      </table>
    </div>

    <header class="header">
      <div></div>
    </header>

    <!-- Page-specific (main) content here -->

    <main class="container">
      <section class="food-type">
        <div class="type-pick">Chicken</div>
        <div class="type-pick">Beef</div>
        <div class="type-pick">Pork</div>
        <div class="type-pick">Fish</div>
        <div class="type-pick">Seafood</div>
        <div class="type-pick">Dessert</div>
        <div class="type-pick">Drinks</div>
      </section>

      <section class="food-card-holder">
        <div class="food-card">
          <section class="food-info">
            <div>
              <img
                class="image"
                src="https://www.bemindfulbehuman.com/wp-content/uploads/MG_9073.deep_.fried_.calamari.final1_1.png"
                alt="Crispy Calamari"
              />
            </div>
            <div class="desc">Crispy Calamari $14.99</div>
          </section>
          <button>Add to cart</button>
        </div>

        <div class="food-card">
          <section class="food-info">
            <div>
              <img
                class="image"
                src="https://iheartumami.com/wp-content/uploads/2019/08/Whole30-Thai-Basil-Chicken-Paleo-Pad-Krapow-Gai-Easy-I-Heart-Umami-500x375.jpg"
                alt="Pad Ga Prow"
              />
            </div>
            <div class="desc">Pad Gra Prow - $14.99</div>
          </section>
          <button>Add to cart</button>
        </div>
      </section>
    </main>

    <script src="public/scripts/cart.js"></script>
    <script src="public/scripts/app.js"></script>

